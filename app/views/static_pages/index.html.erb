<div class="col-md-12 container-fluid">
  <div class="row">
    <%= image_tag('logo.png') %>
    <h1>It Works!</h1>
    <p>Find me in app/views/static_pages/index.html.erb</p>
  </div>
  <div class="row">
    
  </div>
  <div class="row">
    <% @eins.each do |ein| %>
      <%= form_tag("/api/v1/eins/#{ein}", method: "get") do %>
        <%= hidden_field_tag(:ein, value: ein) %>
        <%= submit_tag("Get Data for Tax ID: #{ein}", class:'btn btn-primary') %>
      <% end %>
    <% end %>
  </div>
  <div class="row" id="results">

  </div>
</div>
